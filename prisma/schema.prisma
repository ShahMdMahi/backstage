  ringtonesAccessLevel             RINGTONE_SYSTEM_ACCESS_LEVEL[]              @default([])
  artistsAccessLevel               ARTIST_SYSTEM_ACCESS_LEVEL[]                @default([])
  performersAccessLevel            PERFORMER_SYSTEM_ACCESS_LEVEL[]             @default([])
  producersAndEngineersAccessLevel PRODUCER_AND_ENGINEER_SYSTEM_ACCESS_LEVEL[] @default([])
  writersAccessLevel               WRITER_SYSTEM_ACCESS_LEVEL[]                @default([])
  publishersAccessLevel            PUBLISHER_SYSTEM_ACCESS_LEVEL[]             @default([])
  labelsAccessLevel                LABEL_SYSTEM_ACCESS_LEVEL[]                 @default([])
  transactionsAccessLevel          TRANSACTION_SYSTEM_ACCESS_LEVEL[]           @default([])
  withdrawsAccessLevel             WITHDRAWS_SYSTEM_ACCESS_LEVEL[]             @default([])
  consumptionAccessLevel           CONSUMPTION_SYSTEM_ACCESS_LEVEL[]           @default([])
  engagementAccessLevel            ENGAGEMENT_SYSTEM_ACCESS_LEVEL[]            @default([])
  revenueAccessLevel               REVENUE_SYSTEM_ACCESS_LEVEL[]               @default([])
  geoAccessLevel                   GEO_SYSTEM_ACCESS_LEVEL[]                   @default([])
  rightsManagementAccessLevel      RIGHTS_MANAGEMENT_SYSTEM_ACCESS_LEVEL[]     @default([])
  metadata                         Json?                                       @db.JsonB

  // Enums

  // Relations
  user     User @relation(name: "SystemAccessUser", fields: [userId], references: [id], onDelete: Cascade)
  assigner User @relation(name: "SystemAccessAssigner", fields: [assignerId], references: [id], onDelete: Cascade)

  // Timestamps
  createdAt   DateTime  @default(now())
  updatedAt   DateTime  @updatedAt
  suspendedAt DateTime?
  expiresAt   DateTime

  // Indexes & Constraints
}

