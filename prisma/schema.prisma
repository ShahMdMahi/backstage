  releases                       Release[]
  tracks                         Track[]
  videos                         Video[]
  ringtones                      Ringtone[]
  artists                        Artist[]
  performers                     Performer[]
  producersAndEngineers          ProducerAndEngineer[]
  writers                        Writer[]
  publishers                     Publisher[]
  labels                         Label[]
  transactions                   Transaction[]
  withdrawals                    Withdrawal[]

  // Timestamps
  createdAt          DateTime  @default(now())
